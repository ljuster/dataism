= form_for @algorithm do |f|
  - if @algorithm.errors.any?
    #error_explanation
      %h2= "#{pluralize(@algorithm.errors.count, "error")} prohibited this algorithm from being saved:"
      %ul
        - @algorithm.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :input
    = f.text_field :input
  .actions
    = f.submit 'Run'
